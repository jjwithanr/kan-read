<div class="list-models">
    <h1>Books</h1>

    <table>
        <tr>
            <th>Title</th>
            <th>Author</th>
        </tr>
        {{#each model as |book|}}
        <tr>
            <td>{{book.title}}</td>
            <td>{{book.author}}</td>
        </tr>
        {{/each}}
    </table>
</div>